    !if $(COMPAL_EC_AUTO_FLASH_SUPPORT) == YES
      INF $(COMPAL_COMMON_PATH)/CompalEcAutoFlash/$(COMPAL_EC_AUTO_FLASH_VERSION)/CompalEcAutoFlash.inf
    !endif
    !if $(COMPAL_EC_FLASH_TOOL_SUPPORT) == YES
      INF $(COMPAL_COMMON_PATH)/CompalEcAutoFlash/$(COMPAL_EC_AUTO_FLASH_VERSION)/CompalEcFlash.inf
    !endif
