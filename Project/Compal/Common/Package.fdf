    INF $(COMPAL_COMMON_PATH)/CompalPlatformSmm/$(COMPAL_PLATFORM_SMM_VERSION)/$(COMPAL_CHIPSET_VENDOR)/Smm/Smi/CompalPlatformSmm/CompalPlatformSmm.inf
  !endif
  !if "$(EC_PLATFORM_SETTING)" == "I2C"
    INF $(COMPAL_COMMON_PATH)/CompalEC/$(COMPAL_EC_VERSION)/$(COMPAL_CHIPSET_VENDOR)/$(EC_CHIPSET_VENDOR)/$(EC_CHIPSET_NAME)/CompalECFlashI2C/CompalECFlashI2C.inf
    INF $(COMPAL_COMMON_PATH)/CompalEC/$(COMPAL_EC_VERSION)/$(COMPAL_CHIPSET_VENDOR)/$(EC_CHIPSET_VENDOR)/$(EC_CHIPSET_NAME)/CompalECFwAutoLoadDxe/CompalECFwAutoLoadDxe.inf 
  !endif
  !if $(COMPAL_THERMAL_UTILITY_SUPPORT) == YES
    INF $(COMPAL_COMMON_PATH)/CompalThermalUtility/$(COMPAL_THERMAL_UTILITY_VERSION)/$(COMPAL_CHIPSET_VENDOR)/Smm/Smi/CompalThermalToolSmi/CompalThermalToolSmi.inf
  !endif
  INF $(COMPAL_COMMON_PATH)/CompalCMFC/$(COMPAL_CMFC_VERSION)/$(COMPAL_CHIPSET_VENDOR)/Dxe/CompalCMFCDxe.inf
  INF $(COMPAL_COMMON_PATH)/CompalCMFC/$(COMPAL_CMFC_VERSION)/$(COMPAL_CHIPSET_VENDOR)/Smm/Hook/CompalCMFCSmmHook.inf
  INF $(COMPAL_COMMON_PATH)/CompalCMFC/$(COMPAL_CMFC_VERSION)/$(COMPAL_CHIPSET_VENDOR)/Smm/Smi/CompalCMFCSmmCommon/CompalCMFCCommonSwSmi.inf
  INF $(COMPAL_COMMON_PATH)/CompalCMFC/$(COMPAL_CMFC_VERSION)/$(COMPAL_CHIPSET_VENDOR)/Smm/Smi/CompalCMFCSmmOEM/CompalCMFCOEMSwSmi.inf
